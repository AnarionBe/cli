{
  "name": "@becode/cli",
  "version": "0.0.1",
  "private": true,
  "description": "CLI Utils at BeCode",
  "author": {
    "name": "leny",
    "url": "https://becode.org",
    "email": "leny@becode.org"
  },
  "bin": {
    "becode": "./bin/index.js"
  },
  "scripts": {
    "work": "npx babel src --watch --verbose --out-dir bin",
    "build": "npx babel src --out-dir bin",
    "lint": "npx eslint --fix --cache src"
  },
  "lint-staged": {
    "*.js": [
      "npx eslint --fix --cache src",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/becodeorg/cli.git"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^2.19.0",
    "git-toplevel": "^1.1.1",
    "inquirer": "^6.2.0",
    "opn": "^5.4.0",
    "user-home": "^2.0.0",
    "yn": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.7.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3"
  }
}
